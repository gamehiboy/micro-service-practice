docker rm -f $(docker ps -aq)
docker ps | grep "redis" | awk '{ print $1 }' | xargs docker rm -f

docker build -t 10.5.24.46:80/nscloud/redis:2.8 .

docker run -d --name=mysql -it -p 3306:3306 -p 2222:22 \
    -e TIMEOUT=0 \
    -e TCP_KEEPALIVE=60 \
    -e REQUIRE_PAAS=1 \
    10.5.24.46:80/nscloud/redis:2.8

docker run -d --name=mysql -it -p 3306:3306 -p 2222:22 \
    -e TIMEOUT=0 \
    -e TCP_KEEPALIVE=60 \
    -e REQUIRE_PAAS=1 \
    -e REDIS_PAASWORD=NzEzYTU0OGQ2ZGUwODZjOTk1NWNhNDY4 \
    10.5.24.46:80/nscloud/redis:2.8

docker run -d --name=mysql -it -p 3306:3306 -p 2222:22 \
    -e TIMEOUT=0 \
    -e TCP_KEEPALIVE=60 \
    -e REQUIRE_PAAS=0 \
    10.5.24.46:80/nscloud/redis:2.8
